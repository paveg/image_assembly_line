{
  "name": "image_assembly_line",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Action that runs our docker image build flow.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freee/image_assembly_line.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "freee K.K.",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "@bugsnag/js": "^7.2.1",
    "@slack/bolt": "^2.1.0",
    "@types/uuid": "^8.0.0",
    "aws-sdk": "^2.690.0",
    "axios": "^0.19.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "kind-of": "^6.0.3",
    "minimist": "^1.2.3",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tsconfig-freee": "0.1.1",
    "typescript": "^3.6.4"
  }
}
